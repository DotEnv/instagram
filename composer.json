{
	"name": "dotenv/instagram",
	"description": "A simple inspired by socialite library for laravel to authenticate users and obtain data from instagram api.",
	"type": "library",
	"license": "MIT",
	"keywords": ["php", "laravel", "oauth", "library", "dotenv", "instagram", "social", "social-media", "tiago-perrelli"],
	"authors": [
		{
			"name": "Tiago Perrelli",
		 	"email": "tiagoyg@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"guzzlehttp/guzzle": "~5.0|~6.0",
        "illuminate/contracts": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "4.*"
	},
	"autoload": {
		"psr-4": {
			"Dotenv\\Instagram\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}